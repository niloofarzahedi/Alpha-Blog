# Ignore bundler config
/.bundle

# Ignore the log directory and temp files.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore pid files, and lock files generated by process managers.
*.pid
*.pid.lock

# Ignore SQLite databases if you use them in development.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore system-generated files
/public/system
/public/uploads

# Ignore compiled assets and packs.
/public/assets
/public/packs

# Ignore secrets and credentials that should remain private.
config/master.key
config/credentials.yml.enc

# Ignore node modules, if using a frontend build system.
/node_modules

# Ignore dotenv environment variable files.
/.env
.env.*

# Ignore temporary files created by editors.
*.swp
*.swo
*~

# Ignore IDE-specific files (e.g., RubyMine, VS Code)
.idea/
.vscode/

# Ignore byebug history files
.byebug_history

# Ignore coverage reports
/coverage
